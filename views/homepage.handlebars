<!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<section>
  <a href="/post/{{post.id}}">
    <h2>{{post.title}}</h2>
    <p>By <span class="">{{post.user.name}}</span> on {{format_date post.date_created}}</p>
      <div class="content">
        <p>{{post.content}}</p>
      </div>
  </a>
</section>
{{/each}}

